local project = {
    pre_build = {},
    fast_up_to_date_check = false
}

function project.init(proj, ctx)
    proj.pre_build = {
        "cd " .. _WORKING_DIR,
        "powershell -File " .. _MAIN_SCRIPT_DIR .. "/utils/format/run-clang-format.ps1" -- This is honestly a really bad idea...
    }
    
    return proj
end

return nil